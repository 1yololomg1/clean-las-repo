---
import BaseLayout from "../layouts/BaseLayout.astro";
---
<BaseLayout title="Partners — TraceSeis" description="TraceSeis partners with select industry leaders to deliver geoscience solutions.">
  <section class="section wrap" id="partners">
    <h1>Partners</h1>
    <div class="partners-list">
      <div class="card">
        <h3><a href="https://www.geoinsights.com/" target="_blank" rel="noopener">Geophysical Insights</a></h3>
        <p class="muted">Analytics and machine learning applications for subsurface interpretation.</p>
      </div>
      <div class="card">
        <h3><a href="https://z-terra.com/" target="_blank" rel="noopener">Z‑Terra</a></h3>
        <p class="muted">Integrated seismic imaging software and processing services.</p>
      </div>
      <div class="card">
        <h3><a href="https://www.nmrpetrophysics.com/" target="_blank" rel="noopener">NMR Petrophysics</a></h3>
        <p class="muted">Petrophysical and mineralogical evaluations and related services.</p>
      </div>
      <div class="card">
        <h3><a href="https://www.brtenergy.com/" target="_blank" rel="noopener">BRT Energy Advisors</a></h3>
        <p class="muted">Prospect generation, evaluations, and related services.</p>
      </div>
    </div>
    <p style="margin-top:12px;"><a href="https://www.traceseis.com/partners-page/" target="_blank" rel="noopener">View partners on traceseis.com</a></p>
  </section>
</BaseLayout>
